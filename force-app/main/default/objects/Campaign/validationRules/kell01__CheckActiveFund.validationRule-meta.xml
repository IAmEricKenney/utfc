<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>kell01__CheckActiveFund</fullName>
    <active>true</active>
    <errorConditionFormula>AND( 
ISCHANGED(kell01__Primary_Fund__c), 
NOT(kell01__Primary_Fund__r.kell01__Active__c) 
)</errorConditionFormula>
    <errorDisplayField>kell01__Primary_Fund__c</errorDisplayField>
    <errorMessage>Default Fund must be active.</errorMessage>
</ValidationRule>
